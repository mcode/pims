  },
  "ALLOWED_ORIGIN": {
    "type": "string",
    "default": "*"
  },
  "MONGO_USERNAME": {
    "type": "string",
    "default": "pims-user"
  },

  "MONGO_PASSWORD": {
    "type": "string",
    "default": "pims-pass"
  },

  "MONGO_URL": {
    "type": "string",
    "default": "mongodb://localhost:27017/pims"
